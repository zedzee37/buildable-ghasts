	"entrypoints": {
		"main": [
			"zedzee.github.io.buildableghasts.BuildableGhasts"
		],
		"client": [
			"zedzee.github.io.buildableghasts.BuildableGhastsClient"
		]
	},
	"mixins": [
		"buildable-ghasts.mixins.json",
